{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
          "label": "build",
          "type": "shell",
          "command": ".vscode/cmdls.bat skipListLab skipLabBuild DEBUG"  
        },
        {
            "label": "run_bat",
            "type": "shell",
            "command": ".vscode/cmdls.bat skipListLab skipLabBuild DEBUG",
            "problemMatcher": [
                {
                    "source": "cmake",
                    "fileLocation": [
                        "relative",
                        "${workspaceFolder}\\skipListLab"
                    ],
                    "applyTo": "allDocuments",
                    "owner": "cmake",
                    "pattern": [
                        {
                            "regexp": "CMake (Error) at (.*txt):([0-9]*)[^:]*:",
                            "kind": "location",
                            "file": 2,
                            "location": 3,
                            "severity": 1,
                            "code": 3
                        },
                        {
                            "regexp": "^(.*)$",
                            "message": 1
                        }
                    ]
                },
                {
                    "source": "gtest",
                    "fileLocation": "absolute",
                    "applyTo": "allDocuments",
                    "owner": "gtest",
                    "pattern": [
                    {
                        "regexp": "^(.*.cpp)\\(([0-9]*)\\): (error):\\s*\"([0-9]*)\"$",
                        "kind": "location",
                        "location": 2,
                        "file": 1,
                        "severity": 3,
                        "code": 4
                    },
                    {
                        "regexp": "^(.*)$",
                        "message": 1
                    }
                    ]
                },
                {
                    "source": "cpp",
                    "fileLocation": "absolute",
                    "pattern": [
                        {
                            "regexp": "^(.*)\\((\\d+|\\d+,\\d+|\\d+,\\d+,\\d+,\\d+)\\):\\s*(warning|error)\\s*([A-Z][0-9]*:)\\s*([0-9]*)$$",
                            "file": 1,
                            "location": 2,                                                                                                          
                            "severity": 3,
                            "code": 5
                        },
                        {
                            "regexp": "^(.*)(\\[.*\\]|\\[.*){1,1}\\s*$",
                            "message": 1
                        }
                    ]
                }
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
